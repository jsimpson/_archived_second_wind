- @activity.laps.each_with_index do |lap, index|
  - lap_presenter = LapPresenter.new(lap)
  %tr
    %td= index + 1
    %td= lap_presenter.elapsed_time
    %td= "#{number_with_precision(lap_presenter.total_distance, precision: 2)} miles"
    %td= "#{number_with_precision(lap_presenter.average_pace, precision: 2)} min/mile"
